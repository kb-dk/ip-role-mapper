<included>
    <contextName>avischk-web-qa</contextName>
 
    <appender name="FILE" class="ch.qos.logback.core.rolling.RollingFileAppender">
      <file>${catalina.base}/logs/avischk-web-qa.log</file>
      <rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
        <fileNamePattern>${catalina.base}/logs/avischk-web-qa.%d{yyyy-MM-dd}.log</fileNamePattern>
        <maxHistory>30</maxHistory>
      </rollingPolicy>
 
      <append>true</append>
      <encoder>
        <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level %logger{36} - %msg%n</pattern>
      </encoder>
    </appender>
 
    <root level="INFO">
      <appender-ref ref="FILE" />
    </root>
    <logger name="dk.statsbiblioteket" level="DEBUG" />
 
</included>

